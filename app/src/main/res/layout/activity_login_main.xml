<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="rosarydarling.ailis.LoginMain"
    tools:layout_editor_absoluteY="81dp">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="731dp"
        android:layout_marginStart="14dp"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="14dp"
        android:layout_marginRight="14dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <EditText
            android:id="@+id/txtLogEmail"
            android:layout_width="match_parent"
            android:layout_height="67dp"
            android:ems="10"
            android:hint="Enter Your Email"
            android:inputType="textEmailAddress"
            tools:layout_editor_absoluteX="13dp"
            tools:layout_editor_absoluteY="53dp" />

        <EditText
            android:id="@+id/txtLogPassword"
            android:layout_width="match_parent"
            android:layout_height="67dp"
            android:ems="10"
            android:hint="Enter Your Password"
            android:inputType="textPassword"
            tools:layout_editor_absoluteX="17dp"
            tools:layout_editor_absoluteY="143dp" />

        <Button
            android:id="@+id/btn_rrLogin"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="Regular User Login"
            tools:layout_editor_absoluteX="214dp"
            tools:layout_editor_absoluteY="340dp" />

        <Button
            android:id="@+id/btn_brLogin"
            android:layout_width="match_parent"
            android:layout_height="59dp"
            android:text="Business User Login" />

        <Button
            android:id="@+id/btnRegister"
            android:layout_width="match_parent"
            android:layout_height="67dp"
            android:text="Register"
            tools:layout_editor_absoluteX="41dp"
            tools:layout_editor_absoluteY="341dp" />
    </LinearLayout>

</android.support.constraint.ConstraintLayout>
